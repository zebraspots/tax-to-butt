{
	"manifest_version": 2,
	"name": "Tax To Butt",
	"version": "1.1",
	"description": "Replaces occurrences of tax-related words with butt-related words.",
	"icons": 
	{ 
		"16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png" 
    },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

